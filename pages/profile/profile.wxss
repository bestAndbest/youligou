/* pages/profile/profile.wxss */
page{
  padding:0 36rpx;
   box-sizing: border-box;
   background-color: #FAFAFA;
}
.header{
  display: flex;
  align-items: center;
  width: 100%;
  box-sizing: border-box;
  padding:30rpx 0;
  font-size: 38rpx;
}
.header image{
  width: 140rpx;
  border-radius: 50%;
  margin-right: 30rpx;
}

.start,.order{
  width: 100%;
  display: flex;
  justify-content: space-between;
}
.start-item,.order-item{
  font-size: 32rpx;
  color: #666;
  text-align: center;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}
.order-box,.other{
  margin: 34rpx 0;
  padding: 28rpx;
  box-sizing: border-box;
  width: 100%;
  background-color: #fff;
  border-radius: 20rpx;
}
.other{
  display: flex;
  justify-content: space-between;
  font-size: 34rpx;
   margin: 24rpx 0;
   padding: 28rpx;
}
.myorder{
  font-size: 34rpx;
  padding-bottom: 20rpx;
  margin-bottom: 30rpx;
  border-bottom: 2rpx solid #f4f4f4;
}
.iconfont{
  font-size: 60rpx;
  color: #ff5777;
}